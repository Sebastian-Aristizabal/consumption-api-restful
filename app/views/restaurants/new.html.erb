<%= form_with url: restaurants_path, method: :post do |f| %>
  <%= f.label :name%>
  <%= f.text_field :name %>
  <%= f.label :description %>
  <%= f.text_field :description %>
  <%= f.label :address %>
  <%= f.text_field :address %>
  <%= f.label :city %>
  <%= f.text_field :city %>
  <%= f.label :photo_url %>
  <%= f.text_field :photo_url %>
  <%= f.submit %>
<% end %>
